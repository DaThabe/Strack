<UserControl
    x:Class="Strack.Desktop.UI.View.Dashboard.User.AddUserView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dashboard="clr-namespace:Strack.Desktop.UI.View.Dashboard"
    xmlns:local="clr-namespace:Strack.Desktop.UI.View.Dashboard.User"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <ui:NavigationView x:Name="NavigationView" PaneDisplayMode="Top">
        <ui:NavigationView.MenuItems>

            <ui:NavigationViewItem
                Content="主页"
                Icon="{ui:SymbolIcon Symbol=Bug24}"
                TargetPageType="{x:Type dashboard:DashboardView}" />

        </ui:NavigationView.MenuItems>
    </ui:NavigationView>

</UserControl>
