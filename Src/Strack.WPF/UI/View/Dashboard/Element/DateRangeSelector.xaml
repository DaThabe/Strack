<UserControl
    x:Class="Strack.Desktop.UI.View.Dashboard.Element.DateRangeSelector"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Strack.Desktop.UI.View.Dashboard.Element"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <UniformGrid Columns="5">

        <RadioButton Content="全" GroupName="Group" />
        <RadioButton Content="周" GroupName="Group" />
        <RadioButton Content="月" GroupName="Group" />
        <RadioButton Content="年" GroupName="Group" />
        <Button Content="时间"/>

    </UniformGrid>
</UserControl>
